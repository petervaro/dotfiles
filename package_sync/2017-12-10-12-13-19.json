{
    "references":
    {
        "new_pacman":
        [
            "dunst",
            "feh",
            "leafpad",
            "lxtask",
            "numlockx",
            "nvidia-settings",
            "openbox",
            "raw-thumbnailer",
            "rofi",
            "spectacle",
            "task",
            "thunar",
            "thunar-archive-plugin",
            "thunar-volman",
            "tumbler"
        ],
        "new_pacaur":
        [
            "xiccd"
        ],
        "old":
        [
            "calcurse",
            "plank-theme-arc",
            "plank-theme-numix",
            "plank-theme-orchis-git",
            "plank-theme-pantheon-bzr",
            "vim",
            "xfce4",
            "xfce4-goodies"
        ]
    },
    "upgrade":
    [
        {
            "command": ["sudo", "pacman", "-Rns"],
            "parameters": "old"
        },
        {
            "command": ["sudo", "pacman", "-S", "--needed"],
            "parameters": "new_pacman"
        },
        {
            "command": ["pacaur", "-S", "--needed", "--noedit", "--noconfirm"],
            "parameters": "new_pacaur"
        }
    ],
    "downgrade":
    [
        {
            "command": ["sudo", "pacman", "-Rns"],
            "parameters": "new_pacman"
        },
        {
            "command": ["sudo", "pacman", "-Rns"],
            "parameters": "new_pacaur"
        },
        {
            "command": ["sudo", "pacman", "-S", "--needed"],
            "parameters": "old"
        }
    ]
}
