{
    "references":
    {
        "new_pacman_1":
        [
            "dunst",
            "feh",
            "leafpad",
            "lxtask",
            "numlockx",
            "openbox",
            "raw-thumbnailer",
            "rofi",
            "spectacle",
            "task",
            "thunar",
            "thunar-archive-plugin",
            "thunar-volman",
            "tumbler"
        ],
        "new_pacman_2":
        [
            "nvidia-settings"
        ],
        "new_pacaur":
        [
            "xiccd"
        ],
        "old":
        [
            "calcurse",
            "plank-theme-arc",
            "plank-theme-numix",
            "plank-theme-orchis-git",
            "plank-theme-pantheon-bzr",
            "vim",
            "xfce4",
            "xfce4-goodies"
        ]
    },
    "upgrade":
    [
        {
            "command": ["sudo", "pacman", "-Rns"],
            "parameters": "old"
        },
        {
            "command": ["sudo", "pacman", "-S", "--needed"],
            "parameters": "new_pacman_1"
        },
        {
            "command": ["sudo", "pacman", "-S", "--needed"],
            "parameters": "new_pacman_2",
            "variants":
            [
                "home",
                "work"
            ]
        },
        {
            "command": ["pacaur", "-S", "--needed", "--noedit", "--noconfirm"],
            "parameters": "new_pacaur",
            "variants":
            [
                "home",
                "work"
            ]
        }
    ],
    "downgrade":
    [
        {
            "command": ["sudo", "pacman", "-Rns"],
            "parameters": "new_pacman_1"
        },
        {
            "command": ["sudo", "pacman", "-Rns"],
            "parameters": "new_pacman_2",
            "variants":
            [
                "home",
                "work"
            ]
        },
        {
            "command": ["sudo", "pacman", "-Rns"],
            "parameters": "new_pacaur",
            "variants":
            [
                "home",
                "work"
            ]
        },
        {
            "command": ["sudo", "pacman", "-S", "--needed"],
            "parameters": "old"
        }
    ]
}
