{
    "references":
    {
        "new":
        [
            "viewnior",
            "kruler"
        ],
        "old":
        [
            "gnome-screenruler"
        ]
    },
    "upgrade":
    [
        {
            "command": ["sudo", "pacman", "-S"],
            "parameters": "new"
        },
        {
            "command": ["sudo", "pacman", "-Rns"],
            "parameters": "old"
        },
    ],
    "downgrade":
    [
        {
            "command": ["sudo", "pacman", "-Rns"],
            "parameters": "new"
        },
        {
            "command": ["pacaur", "-S", "--noedit", "--noconfirm"],
            "parameters": "old"
        }
    ]
}
