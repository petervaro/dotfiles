        "backup_home_folder_service",
        {
            "variants":
            {
                "home": "backup.service",
                "work": null,
                "mobile": null
            },
            "path": "/etc/systemd/system",
            "sudo": true
        }
    ],
    [
        "backup_home_folder_timer",
        {
            "variants":
            {
                "home": "backup.timer",
                "work": null,
                "mobile": null
            },
            "path": "/etc/systemd/system",
            "sudo": true,
            "post":
            {
                "sudo": true,
                "commands":
                [
                    ["systemctl", "enable", "backup.timer"],
                    ["systemctl", "start", "backup.timer"]
                ]
            }
        }
